<div class="{{ "social" if pageId === 'home' else "social bottom" }}">
  {% for name, soc in social %}
    <a href="{{ soc.link }}" target="_blank" rel="noopener noreferrer" title="{{ soc.title }}">
      <svg xmlns="http://www.w3.org/2000/svg" fill="none" stroke="#000" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" viewbox="0 0 25 25">
        <path d="{{ soc.path }}"/>
      </svg>
    </a>
  {% endfor %}
</div>
